                        <DataGridTextColumn Header="lgD"  Binding="{Binding lgD}"></DataGridTextColumn>
                        <DataGridTextColumn Header="exp"  Binding="{Binding exp}"></DataGridTextColumn>
                        <DataGridTextColumn Header="errorexp"  Binding="{Binding errorexp}"></DataGridTextColumn>
                        <DataGridTextColumn Header="sim"   Binding="{Binding sim}"></DataGridTextColumn>
                        <DataGridTextColumn Header="errorsim"  Binding="{Binding errorsim}"></DataGridTextColumn>
                        <DataGridTextColumn Header="otn"  Binding="{Binding otn}"></DataGridTextColumn>
