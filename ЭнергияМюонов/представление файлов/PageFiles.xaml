                        <DataGridTextColumn Header="errorsim"  Binding="{Binding errorsim}"></DataGridTextColumn>
                        <DataGridTextColumn Header="otn"  Binding="{Binding otn}"></DataGridTextColumn>
                        <DataGridTextColumn Header="otnerror"   Binding="{Binding otnerror}"></DataGridTextColumn>
                    </DataGrid.Columns>
                </DataGrid>

            </TabItem>
            <TabItem Header="EnDepotnD6575">
